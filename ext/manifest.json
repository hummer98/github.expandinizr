{
  "name": "GitHub.Expandinizr",
  "version": "1.5.1",
  "manifest_version": 2,
  "description": "Chrome extension that improves the GitHub experience",
  "homepage_url": "https://github.com/hummer98/github.expandinizr",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/*", "https://ghe.misosiru.io/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*", "https://ghe.misosiru.io/*"
      ],
      "css": [
        "content/inject.css"
      ],
      "js": [
        "content/jquery-2.1.0.min.js",
        "content/inject.js"
      ],
      "run_at": "document_end"
    }
  ]
}
